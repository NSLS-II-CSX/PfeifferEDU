#
#  Pump ON/OFF Commands
#

record(bo, "$(Sys)$(Dev)Cmd:On-Cmd"){
	field(DTYP, "stream")
	field(OUT,  "@pfeiffer.proto writePumpOn($(ID)) $(PORT)")
}

record(bo, "$(Sys)$(Dev)Cmd:Off-Cmd"){
	field(DTYP, "stream")
	field(OUT,  "@pfeiffer.proto writePumpOff($(ID)) $(PORT)")
}

record(bo, "$(Sys)$(Dev)Cmd:OffVent-Cmd"){
	field(DTYP, "stream")
	field(OUT,  "@pfeiffer.proto writePumpOffVent($(ID)) $(PORT)")
}

record(bi, "$(Sys)$(Dev)Pwr-Sts__"){
	field(DTYP, "stream")
	field(INP,  "@pfeiffer.proto readBoolParam($(ID),010) $(PORT)")
