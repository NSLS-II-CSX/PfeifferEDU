#
#  Pump ON/OFF Commands
#

record(bo, "$(Sys)$(Dev)Pwr-Sel"){
	field(DTYP, "stream")
	field(OUT,  "@pfeiffer.proto writeBoolParam($(ID),010) $(PORT)")
	field(FLNK, "$(Sys)$(Dev)Pwr-Sts")
	field(ZNAM, "OFF")
	field(ONAM, "ON")
}

record(bi, "$(Sys)$(Dev)Pwr-Sts__"){
	field(DTYP, "stream")
	field(INP,  "@pfeiffer.proto readBoolParam($(ID),010) $(PORT)")
}

record(bi, "$(Sys)$(Dev)Val:Spd-Sts"){
	field(DTYP, "stream")
	field(INP,  "@pfeiffer.proto readBoolParam($(ID),306) $(PORT)")
}

record(bi, "$(Sys)$(Dev)Val:Acc-Sts"){
	field(DTYP, "stream")
	field(INP,  "@pfeiffer.proto readBoolParam($(ID),307) $(PORT)")
}

record(longin, "$(Sys)$(Dev)S-I"){
	field(DTYP, "stream")
	field(INP,  "@pfeiffer.proto readIntParam($(ID),309) $(PORT)")
	field(EGU,  "Hz")
}

record(longin, "$(Sys)$(Dev)S-RB"){
	field(DTYP, "stream")
	field(INP,  "@pfeiffer.proto readIntParam($(ID),308) $(PORT)")
	field(SCAN, "$(SCAN)")
	field(EGU,  "Hz")
}

record(calc, "$(Sys)$(Dev)Pwr-Sts_"){
	field(INPA, "$(Sys)$(Dev)Pwr-Sts__ PP NMS")
	field(INPB, "$(Sys)$(Dev)Val:Spd-Sts PP NMS")
	field(INPC, "$(Sys)$(Dev)Val:Acc-Sts PP NMS")
	field(INPD, "$(Sys)$(Dev)S-I PP NMS")
	field(CALC, "A + (B << 1) + (C << 2) + ((D > 20) << 3)")
}

record(mbbi, "$(Sys)$(Dev)Pwr-Sts"){
	field(DTYP, "Raw Soft Channel")
	field(INP, "$(Sys)$(Dev)Pwr-Sts_ PP NMS")
	field(ZRST, "OFF")
	field(ZRVL, "0")
	field(ONST, "AT SPEED")
	field(ONVL, "11")
	field(TWST, "ACCELERATING")
	field(TWVL, "13")
	field(THST, "ACCELERATING")
	field(THVL, "5")
	field(FRST, "DECELERATING")
	field(FRVL, "10")
	field(FVST, "DECELERATING")
	field(FVVL, "8")
	field(ZRSV, "MINOR")
	field(ONSV, "NO_ALARM")
	field(TWSV, "MINOR")
	field(THSV, "MINOR")
	field(FRSV, "MINOR")
	field(FVSV, "MINOR")
	field(SCAN, "$(SCAN)")
}

