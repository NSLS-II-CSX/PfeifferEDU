#
# Records for accessory outputs
#

record(mbbi, "$(Sys)$(Dev)Mode:A1-Sts"){
	field(DTYP, "stream")
	field(INP,  "@pfeiffer.proto readShortIntParam($(ID),035) $(PORT)")
	field(ZRST, "Continuous Fan")
	field(ONST, "Venting Valve")
	field(TWST, "Heating")
	field(THST, "Backing Pump")
	field(FRST, "Temp. Controlled Fan")
	field(FVST, "Sealing Gas")
	field(SXST, "Always 0")
	field(SVST, "Always 1")
	field(EIST, "Power Fail Vent Unit")
	field(SCAN, "10 second")
}

record(mbbo, "$(Sys)$(Dev)Mode:A1-Sel"){
	field(DTYP, "stream")
	field(OUT,  "@pfeiffer.proto writeShortIntParam($(ID),035) $(PORT)")
	field(ZRST, "Continuous Fan")
	field(ONST, "Venting Valve")
	field(TWST, "Heating")
	field(THST, "Backing Pump")
	field(FRST, "Temp. Controlled Fan")
	field(FVST, "Sealing Gas")
