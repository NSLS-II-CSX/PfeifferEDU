record(calc, "$(Sys)$(Dev)Pwr-Sts_"){
	field(INPA, "$(Sys)$(Dev)Pwr-Sts__ PP NMS")
	field(INPB, "$(Sys)$(Dev)Val:Spd-Sts PP NMS")
	field(INPC, "$(Sys)$(Dev)Val:Acc-Sts PP NMS")
	field(INPD, "$(Sys)$(Dev)Val:RotSpd-I PP NMS")
	field(CALC, "A + (B << 1) + (C << 2) + ((D > 20) << 3)")
}

