#
# Extra records for TC400 and above
#

record(mbbi, "$(Sys)$(Dev)Out:Rly1-Sts"){
	field(DTYP, "stream")
	field(INP,  "@pfeiffer.proto readShortIntParam($(ID),045) $(PORT)")
	field(SCAN, "10 second")
	field(ZRST, "Speed SP")
	field(ONST, "No Error")
	field(TWST, "Error")
	field(THST, "Warning")
	field(FRST, "Error or Warning")
	field(FVST, "At Speed")
	field(SXST, "Pump On")
	field(SVST, "Pump Acelerating")
	field(EIST, "Pump Decelerating")
	field(NIST, "Always 0")
	field(TEST, "Always 1")
	field(ELST, "Remote Priority")
	field(TVST, "Heating")
	field(TTST, "Backing Pump")
	field(FTST, "Sealing Gas")
	field(FFST, "Pumping Station")
}

record(mbbo, "$(Sys)$(Dev)Out:Rly1-Sel"){
	field(DTYP, "stream")
	field(OUT,  "@pfeiffer.proto writeShortIntParam($(ID),045) $(PORT)")
